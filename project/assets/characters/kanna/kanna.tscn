resource_name = "KannaSignalManager"
script/source = "extends Node

var _character: Character
var _phase: int = 1
var _phase_transition_on_next_state: bool = false

func _ready():
	await owner.ready
	_character = owner
	_character.stat_changed.connect(_on_stat_change)
	
	_character.get_state(\"SlamDown\").hit_confirmed.connect(func():
		_character.change_stat_by(CharacterStats.Types.MP, 5)
	)
	_character.get_attack(\"SlamDown\").get_node(\"FlameGeyserSpawner\").spawned.connect(_on_flame_geyser_spawn)
	_character.get_attack(\"FlameGeysers\").get_node(\"FlameGeyserSpawner\").spawned.connect(_on_flame_geyser_spawn)
	_character.get_attack(\"FlameGeysers\").get_node(\"FlameGeyserSpawner2\").spawned.connect(_on_flame_geyser_spawn)
	for child in _character.get_attack(\"FlameGeysers2\").get_node(\"Attack Set 1\").get_children():
		if child.name.contains(\"FlameGeyserSpawner\"):
			child.spawned.connect(_on_flame_geyser_spawn)
	for child in _character.get_attack(\"FlameGeysers2\").get_node(\"Attack Set 2\").get_children():
		if child.name.contains(\"FlameGeyserSpawner\"):
			child.spawned.connect(_on_flame_geyser_spawn)
	_character.get_attack(\"FlameGeysers2\").get_node(\"FlameGeyserSpawner\").spawned.connect(_on_flame_geyser_spawn)
	_character.get_attack(\"AttackAndSlamDown\").get_node(\"FlameGeyserSpawner\").spawned.connect(_on_flame_geyser_spawn)
	_character.get_attack(\"TripleGeyser\").get_node(\"FlameGeyserSpawner\").spawned.connect(_on_flame_geyser_spawn)
	
	_character.state_changed.connect(_on_state_changed)
	_character.input_state_changed.connect(_on_input_state_changed)
	
	_phase = 1
	_phase_transition_on_next_state = false


func _on_stat_change(type: CharacterStats.Types, new_value: float, old_value: float, max_value: float) -> void:
	if type == CharacterStats.Types.HP:
		var percentLeft = new_value / max_value
		match _phase:
			1:
				if percentLeft <= 0.75:
					_phase_transition_on_next_state = true
					_phase = 2
			2:
				if percentLeft <= 0.4:
					_phase_transition_on_next_state = true
					_phase = 3
			_:
				pass


func _on_state_changed(to: State, from: State) -> void:
	var input_state_machine := _character.get_input_state_machine() as StateMachine
	if to.name == \"Idle\":
		input_state_machine.reset_state_name = \"Wait\"
		input_state_machine.reset()
	elif to.name == \"Revenge\":
		input_state_machine.reset_state_name = \"NoInput\"
		input_state_machine.reset()
	elif _phase_transition_on_next_state:
		_phase_transition_on_next_state = false
		input_state_machine.reset_state_name = \"NoInput\"
		input_state_machine.reset()
		match _phase:
			1:
				pass
			2: 
				_character.request_state_transition(\"FlyUp\")
				_character.get_state(\"Revenge\").next_state = \"TripleGeyser\"
			3: 
				_character.request_state_transition(\"FlyUp2\")
				_character.get_state(\"Revenge\").next_state = \"TripleGeyser\"
			_:
				pass


func _on_input_state_changed(to: State, from: State) -> void:
	if to.name == \"Attack\":
		_character.face_target()
		match _phase:
			1:
				_character.request_state_transition(\"SpinAttack\")
			2: 
				_character.request_state_transition(\"AttackAndSlamDown\")
			3: 
				_character.request_state_transition(\"AttackAndSlamDown\")
			_:
				_character.request_state_transition(\"Attack\")


func _on_flame_geyser_spawn(flame_geyser) -> void:
	var hitbox := flame_geyser.get_node(\"Hitbox\") as Hitbox
	hitbox.set_character(_character)
	hitbox.set_team(_character.team)
	hitbox.hit.connect(func(_hit_character):
		_character.change_stat_by(CharacterStats.Types.MP, 5)
		)
"

